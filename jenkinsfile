pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
ssh "mvn clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
ssh "mvn test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
ssh "mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
